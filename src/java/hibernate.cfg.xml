<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/DatawareHouse?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">root</property>
    <mapping resource="model/pojo/Control.hbm.xml"/>
    <mapping resource="model/pojo/Usuario.hbm.xml"/>
    <mapping resource="model/pojo/RangoEdades.hbm.xml"/>
    <mapping resource="model/pojo/Entidad.hbm.xml"/>
    <mapping resource="model/pojo/Escuela.hbm.xml"/>
    <mapping resource="model/pojo/NivelEducativo.hbm.xml"/>
    <mapping resource="model/pojo/TipoUsuario.hbm.xml"/>
    <mapping resource="model/pojo/Poblacion.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
